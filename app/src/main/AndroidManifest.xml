<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="am.tsd.inchuner.program26">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="am.tsd.inchuner.program26.MainActivity"
            android:screenOrientation="sensorLandscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="am.tsd.inchuner.program26.activities.GameActivity"
            android:screenOrientation="sensorLandscape"
            android:theme="@style/AppTheme" />

        <activity
            android:name="am.tsd.inchuner.program26.activities.GameOverActivity"
            android:screenOrientation="sensorLandscape"
            android:theme="@style/AppTheme" />
    </application>

</manifest>